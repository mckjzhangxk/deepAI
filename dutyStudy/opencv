#Note remove DOPENCV_EXTRA_MODULES_PATH is not necessary!!!!!
https://blog.csdn.net/howlclat/article/details/78819760

cmake -DCMAKE_BUILD_TYPE=Release -DBUILD_DOCS=ON -DBUILD_EXAMPLES=ON -DCMAKE_INSTALL_PREFIX=/home/zhangxk/cv2 -DOPENCV_EXTRA_MODULES_PATH=/home/zhangxk/opencv_contrib -DPYTHON3_EXECUTABLE=/usr/bin/python3 -DPYTHON_INCLUDE_DIR=/usr/include/python3.5 -DPYTHON_INCLUDE_DIR2=/usr/include/x86_64-linux-gnu/python3.5m -DPYTHON_LIBRARY=/usr/lib/x86_64-linux-gnu/libpython3.5m.so -DPYTHON3_NUMPY_INCLUDE_DIRS=/usr/local/lib/python3.5/dist-packages/numpy/core/include /home/zhangxk/opencv-3.4.3

cmake -DWITH_QT=ON -DWITH_OPENGL=ON -DFORCE_VTK=ON -DWITH_TBB=ON -DWITH_GDAL=ON -DWITH_XINE=ON -DBUILD_EXAMPLES=ON


cmake -DCMAKE_BUILD_TYPE=Release -DCMAKE_INSTALL_PREFIX=/usr/local/lib/python3.5/dist-packages/cv2 /home/zhangxk/cv/opencv/ -DCMAKE_INSTALL_PREFIX=/usr/local/lib/python3.5/dist-packages/cv2  -DPYTHON3_EXECUTABLE=/usr/bin/python3 -DPYTHON_INCLUDE_DIR=/usr/include/python3.5 -DPYTHON_INCLUDE_DIR2=/usr/include/x86_64-linux-gnu/python3.5m -DPYTHON_LIBRARY=/usr/lib/x86_64-linux-gnu/libpython3.5m.so -DPYTHON3_NUMPY_INCLUDE_DIRS=/usr/local/lib/python3.5/dist-packages/numpy/core/include /home/zhangxk/cv/opencv/



